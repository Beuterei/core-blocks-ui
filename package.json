{
  "name": "@beuluis/core-blocks-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "types": "./dist/main.d.ts"
    },
    "./twind-config": {
      "import": "./dist/twind.config.js",
      "types": "./dist/twind.config.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "storybook": "storybook dev --config-dir storybook -p 6006",
    "build:storybook": "storybook build --config-dir storybook"
  },
  "peerDependencies": {
    "react": "^18"
  },
  "dependencies": {
    "@emotion/react": "11.11.4",
    "@emotion/styled": "11.11.5",
    "@radix-ui/react-checkbox": "^1.1.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-select": "^2.1.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@twind/core": "1.1.3",
    "@twind/preset-autoprefix": "1.0.7",
    "@twind/preset-tailwind": "1.1.4",
    "@twind/with-react": "1.1.3",
    "class-variance-authority": "^0.7.0",
    "lucide-react": "^0.395.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@beuluis/eslint-config": "2.0.1",
    "@faker-js/faker": "8.4.1",
    "@storybook/addon-a11y": "8.1.11",
    "@storybook/addon-actions": "8.1.11",
    "@storybook/addon-essentials": "8.1.11",
    "@storybook/addon-themes": "8.1.11",
    "@storybook/addon-viewport": "8.1.11",
    "@storybook/blocks": "8.1.11",
    "@storybook/react": "8.1.11",
    "@storybook/react-vite": "8.1.11",
    "@storybook/test": "8.1.11",
    "@types/node": "20.14.2",
    "@types/react": "18.2.66",
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@typescript-eslint/parser": "7.2.0",
    "@vitejs/plugin-react": "4.2.1",
    "autoprefixer": "10.4.19",
    "eslint": "8.57.0",
    "eslint-plugin-storybook": "0.8.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.7",
    "react": "18.3.1",
    "storybook": "8.1.11",
    "typescript": "5.5.3",
    "vite": "5.2.0",
    "vite-plugin-dts": "3.9.1",
    "vite-tsconfig-paths": "4.3.2"
  },
  "overrides": {
    "@twind/core": {
      "typescript": "$typescript"
    },
    "@twind/preset-autoprefix": {
      "typescript": "$typescript"
    },
    "@twind/preset-tailwind": {
      "typescript": "$typescript"
    },
    "@twind/with-react": {
      "typescript": "$typescript"
    }
  }
}
